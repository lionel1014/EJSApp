<!DOCTYPE html>
<html>
  <%- include('parcials/head.ejs') %>
  <body>
    <!-- Navbar (sit on top) -->
    <%- include('parcials/navbar.ejs') %>
    <!-- Header -->
    <%- include('parcials/header.ejs') %>
    <!-- Page content -->
    <div class="ps-content">

      <!-- Menu Section -->
      <div class="ps-row ps-padding-64" id="menu">
        <div class="ps-col l6 ps-padding-large">
          <h1 class="ps-center" ><%= product.nombre %> </h1>
          <br />
          <h4> <%= product.descripcion %> </h4>
          <p class="ps-text-grey">
            <%= product.precio %>
          </p>
        </div>

        <div class="ps-col l6 ps-padding-large ps-img-menu">
          <img
            src=<%= product.imagen %>
            class="ps-round ps-image ps-opacity-min"
            alt="Menu"
          />
        </div>
      </div>

      <!-- Footer -->
      <%- include('parcials/footer.ejs') %>
    </div>
  </body>
</html>
